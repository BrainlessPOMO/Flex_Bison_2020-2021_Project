PROGRAM input

%function/if test
FUNCTION smaller(INTEGER x1, INTEGER x2)
    IF (x1<x2) THEN
        PRINT(""[,x1]);      
    ELSEIF (x1<x2)
        PRINT(""[,x2]);      
    ELSE 
        PRINT("The two numbers are equal");  
    ENDIF
    RETURN x1;
END_FUNCTION 

%function/while test 
FUNCTION doStuff(INTEGER var1, INTEGER var2)
    WHILE(var1<10 AND var2<20)
        IF(var1==var2) THEN
            BREAK;
        ENDIF        
        var1 = 45;
        var2 >= 56;
    ENDWHILE

    RETURN var2;
END_FUNCTION

%functon/switch case test 

FUNCTION swissFiss(INTEGER day)
    SWITCH(<day>)
    CASE(<11>)
        PRINT("Monday");
        BREAK;
    CASE(<12>)
        PRINT("Tuesday");
        BREAK;
    CASE(<13>)
        PRINT("Wednesday");
        BREAK;
    CASE(<14>)
        PRINT("Thursday");
        BREAK;
    CASE(<15>)
        PRINT("Friday");
        BREAK;
    CASE(<16>)
        PRINT("Saturday");
        BREAK;
    CASE(<17>)
        PRINT("Sunday");
        BREAK;
    ENDSWITCH
END_FUNCTION

%main function

STARTMAIN
    VARS
